#include <iostream>
#define REP(i, n) for (int i = 0; i < (n); i++)
using namespace std;

void solve(long long N, long long A, long long B){
    int ans = 0;

    REP(i, N + 1) {
        if (i == 0) continue ;
        string num = to_string(i);
        int sum = 0;
        REP(j, num.size())
            sum += num[j] - '0';
        if (sum >= A && sum <= B)
            ans += i;
    }

    cout << ans << '\n';
}

int main(){
    cin.tie(0);
    ios::sync_with_stdio(false);
    long long N;
    scanf("%lld",&N);
    long long A;
    scanf("%lld",&A);
    long long B;
    scanf("%lld",&B);
    solve(N, A, B);
    return 0;
}
